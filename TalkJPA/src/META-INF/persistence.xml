<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="TalkJPA" transaction-type="RESOURCE_LOCAL">
		<class>model.Korisnik</class>
		<class>model.Kurs</class>
		<class>model.Lekcija</class>
		<class>model.Odgovor</class>
		<class>model.Pitanje</class>
		<class>model.Prijavljenipolaznik</class>
		<class>model.Resenje</class>
		<class>model.Test</class>
		<class>model.Uloga</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pris-er?useUnicode=true&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
